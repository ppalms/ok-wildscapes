{
  "name": "cdk",
  "version": "0.1.0",
  "license": "GPL-3.0-or-later",
  "bin": {
    "cdk": "esbuild.out/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "build:cjs": "node esbuild.cjs.config.js",
    "package": "conc \"tsc --noEmit\" \"yarn build:cjs\"",
    "deploy": "yarn package && cdk deploy OkWildscapesStack --profile ok-wildscapes --verbose --output-file outputs.json"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "20.9.0",
    "@types/source-map-support": "^0.5.10",
    "aws-cdk": "2.106.1",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "@aws-cdk/aws-amplify-alpha": "^2.106.1-alpha.0",
    "aws-cdk-lib": "2.106.1",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
